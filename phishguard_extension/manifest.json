{
  "manifest_version": 3,
  "name": "Windows PhishGuard Gmail Scanner",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["http://127.0.0.1:5000/*"],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"],
      "permissions": ["notifications", "activeTab", "scripting"]
    }
  ]
}
